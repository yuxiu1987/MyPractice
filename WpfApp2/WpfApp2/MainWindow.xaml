<Window x:Class="WpfApp2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" Loaded="Window_Loaded">
    <Grid>
        <Label x:Name="label" Content="{Binding namelist[0].name}" HorizontalAlignment="Left" Height="23" Margin="201,121,0,0" VerticalAlignment="Top" Width="54"/>
        <Label x:Name="label1" Content="{Binding namelist[1].name}" HorizontalAlignment="Left" Height="23" Margin="201,161,0,0" VerticalAlignment="Top" Width="54"/>
        <Label x:Name="label2" Content="{Binding namelist[2].name}" HorizontalAlignment="Left" Height="23" Margin="201,191,0,0" VerticalAlignment="Top" Width="54"/>
        <Button x:Name="button" Content="change" HorizontalAlignment="Left" Height="29" Margin="224,239,0,0" VerticalAlignment="Top" Width="108" Click="button_Click"/>

    </Grid>
</Window>
